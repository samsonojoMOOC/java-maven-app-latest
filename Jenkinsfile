pipeline {   
    agent any
    stages {
        stage("init") {
            steps {
                echo "initializing the script"
            }
        }
        stage("build jar") {
            steps {
                echo "building the jar file"
            }
        }

        stage("build image") {
            steps {
            echo "building the image"
            }
        }

        stage("deploy") {
            steps {

                echo "deploying the script"

            }
        }               
    }
} 
